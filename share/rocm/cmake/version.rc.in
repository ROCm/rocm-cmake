1 VERSIONINFO
FILEVERSION     @ROCM_DLL_VERSION_COMMA@
PRODUCTVERSION  @ROCM_DLL_VERSION_COMMA@
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
      VALUE "FileVersion", "@ROCM_DLL_VERSION@\0"
      VALUE "ProductVersion", "@ROCM_DLL_VERSION@\0"
    END
  END
  /* For some reason the ProductVersion would not appear unless I add */
  /* the following section: VarFileInfo */
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1252
  END
END
1 VERSIONINFO
FILEVERSION     @DLL_FILE_VERSION_COMMA@
PRODUCTVERSION  @DLL_PRODUCT_VERSION_COMMA@
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
      VALUE "FileVersion", "@DLL_FILE_VERSION@\0"
      VALUE "ProductVersion", "@DLL_PRODUCT_VERSION@\0"
    END
  END
  /* For some reason the ProductVersion would not appear unless I add */
  /* the following section: VarFileInfo */
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1252
  END
END